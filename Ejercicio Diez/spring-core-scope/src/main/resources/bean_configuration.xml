<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!-- bean definitions here -->
    <bean id="persona" class="dgtic.core.modelo.Persona"
                  scope="prototype">
        <property name="nombre" value="Raul"/>
        <property name="edad" value="34"/>
        <property name="computadora" ref="computadora"/>
        <property name="direccion" ref="direccion"/>
    </bean>

    <bean id="direccion" class="dgtic.core.modelo.Direccion"
          scope="prototype">

    </bean>
    
    
</beans>